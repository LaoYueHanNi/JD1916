package com.briup.ch05;

public class Circle extends Shape {
	private  int radius;
	private int x,y;//点
	
	public Circle(int x,int y,int radius) {
		super(x,y);
		setRadius(radius);
	}
	
	public void setRadius(int radius) {
		this.radius = radius;
	}
	
	@Override
	public void draw() {
		System.out.println("画一圆，圆心在("+x+","+y+")"+"半径是"+radius);
	}
}

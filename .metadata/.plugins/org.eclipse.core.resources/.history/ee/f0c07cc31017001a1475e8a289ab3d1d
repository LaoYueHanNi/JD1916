package n1_100;

public class N34 {
	public int[] searchRange(int[] nums, int target) {
		int [] result = {-1,-1};
		for(int i = 0; i <nums.length; i++) {
			if(nums[i]==target) {
				result[0]=i;
				for(int j = nums.length-1;j>=0;j--) {
					if(nums[j]==target) {
						right[1] = j;
						
					}
				}
			}
		}
		return null;
	}
}

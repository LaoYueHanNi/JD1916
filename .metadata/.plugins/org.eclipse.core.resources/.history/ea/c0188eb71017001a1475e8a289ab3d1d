package n1_100;

public class N34 {
	public int[] searchRange(int[] nums, int target) {
		int left = -1;
		int right = -1;
		int [] result = new int[2];
		for(int i = 0; i <nums.length; i++) {
			if(nums[i]==target) {
				left=i;
				result[0]=i;
				for(int j = nums.length-1;j>=0;j--) {
					if(nums[j]==target) {
						right = j;
						
					}
				}
			}
		}
		return null;
	}
}

package com.briup.ch05;

public class Shape {
	private  int radius;
	private int x,y;//µã'
	public void draw() {
		
	}
	
	public Shape(int x,int y){
		setX(x);
		setY(y);
	}

	public void setX(int x) {
		this.x=x;
	}
	public void setY(int y) {
		this.y=y;
	}
	public void setRadius(int radius) {
		this.radius = radius;
	}
	public void draw(String title) {
		System.out.println("title:"+title);
		draw();
	}
}
